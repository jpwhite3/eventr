    "sockjs-client": "^1.6.1",
    "web-vitals": "^4.2.4",
    "xlsx": "^0.18.5"
  },
  "scripts": {
    "start": "PORT=3002 react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "test:ci": "react-scripts test --coverage --watchAll=false",
    "eject": "react-scripts eject"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "jest": {
    "moduleNameMapper": {
      "^react-markdown$": "<rootDir>/src/__mocks__/react-markdown.js",
      "^axios$": "<rootDir>/src/__mocks__/axios.js",
      "^../api/apiClient$": "<rootDir>/src/__mocks__/apiClient.js",
      "^../hooks/useAuth$": "<rootDir>/src/__mocks__/useAuth.ts"
    },
    "collectCoverageFrom": [
      "src/**/*.{js,jsx,ts,tsx}",
      "!src/**/*.d.ts",
      "!src/index.tsx",
      "!src/reportWebVitals.ts",
      "!src/__mocks__/**"
    ],
    "coverageReporters": ["text", "lcov", "html"]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@types/jest": "^29.5.14",
    "@types/node": "^22.19.10",
    "@types/react": "^18.3.28",
    "@types/react-dom": "^18.3.7",
    "typescript": "^4.9.5"
  },
  "overrides": {
    "nth-check": ">=2.0.1",
    "postcss": ">=8.4.31", 
    "svgo": ">=2.0.0",
    "css-select": ">=4.0.0",
    "resolve-url-loader": ">=5.0.0",
    "glob": ">=10.0.0",
    "rimraf": ">=5.0.0"
  }
}
