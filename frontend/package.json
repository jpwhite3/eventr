    "@testing-library/react": "^16.3.2",
    "@testing-library/user-event": "^14.6.1",
    "axios": "^1.13.5",
    "bootstrap": "^5.3.8",
    "chart.js": "^4.5.0",
    "date-fns-tz": "^3.2.0",
    "html2canvas": "^1.4.1",
    "html2pdf.js": "^0.12.0",
    "jspdf": "^3.0.2",
    "jsqr": "^1.4.0",
    "moment-timezone": "^0.6.0",
    "papaparse": "^5.5.3",
    "qrcode.react": "^4.2.0",
    "rc-slider": "^11.1.8",
